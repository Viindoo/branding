<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="iap.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//a[@href='https://www.odoo.com/documentation/14.0/applications/general/in_app_purchase/in_app_purchase.html']"
            position="attributes"> 
                <attribute name="href">https://viindoo.com/pricing</attribute>
            </xpath>
            <xpath expr="//a[@href='https://www.odoo.com/documentation/14.0/developer/webservices/iap.html']"
            position="attributes"> 
                <attribute name="href">https://viindoo.com/documentation/14.0/applications/getting-started/system-settings/steps-to-create-viindoo-instance.html</attribute>
            </xpath>
        </field>
    </record>
</odoo>
