<odoo>
    <record id="acquirer_form" model="ir.ui.view">
	    <field name="name">payment.acquirer.form</field>
	    <field name="model">payment.acquirer</field>
	    <field name="inherit_id" ref="payment.acquirer_form"/>
	    <field name="arch" type="xml">
	       <xpath expr="//div[hasclass('oe_title')]/div/a" position="attributes">
	           <attribute name="href">https://viindoo.com/pricing?utm_source=db&amp;utm_medium=module</attribute>
	       </xpath>
	    </field>
	</record>
	
    <record id="acquirer_kanban" model="ir.ui.view">
	    <field name="name">payment.acquirer.kanban</field>
	    <field name="model">payment.acquirer</field>
	    <field name="inherit_id" ref="payment.acquirer_kanban"/>
	    <field name="arch" type="xml">
	       <xpath expr="//div[hasclass('o_payment_acquirer_bottom')]/t/button" position="attributes">
	           <attribute name="href">https://viindoo.com/pricing?utm_source=db&amp;utm_medium=module</attribute>
	       </xpath>
	   </field>
	</record>
</odoo>
