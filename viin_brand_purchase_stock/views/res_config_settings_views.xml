<odoo>
	<record id="res_config_settings_view_form_purchase" model="ir.ui.view">
	    <field name="name">res.config.settings.view.form.stock</field>
	    <field name="model">res.config.settings</field>
	    <field name="inherit_id" ref="purchase_stock.res_config_settings_view_form_purchase"/>
	    <field name="arch" type="xml">
            <xpath expr="//div[@name='request_vendor_setting_container']/div/div/a" position="attributes">
                <attribute name="href">https://viindoo.com/documentation/14.0/applications/supply-chain/inventory/warehouse-management/delivery-orders/delivery-directly-from-suppliers-to-customers-drop-ship.html</attribute>
            </xpath>
        </field>
    </record>
    
    <record id="res_config_settings_view_form_stock" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.stock</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="purchase_stock.res_config_settings_view_form_stock"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='schedule_receivings_setting_container']/div/a" position="attributes">
                <attribute name="href">https://viindoo.com/documentation/14.0/applications/supply-chain/inventory/warehouse-management/planning/understand-the-scheduled-delivery-date-computed-rule.html</attribute>
            </xpath>
        </field>
    </record>
</odoo>
