<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <template id="users_page_main" inherit_id="website_profile.users_page_main" name="Viindoo Users Page">
            <xpath expr="//t[@t-set='body_classname']" position="after">
                <t t-set="body_classname" t-value="'o_wprofile_body'"/>
            </xpath>
        </template>

        <template id="user_profile_main" inherit_id="website_profile.user_profile_main" name="Profile Page">
            <xpath expr="//t[@t-set='body_classname']" position="after">
                <t t-set="body_classname" t-value="'o_wprofile_body'"/>
            </xpath>
        </template>
    </data>
</odoo>
